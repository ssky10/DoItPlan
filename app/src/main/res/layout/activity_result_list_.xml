<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/cardview_light_background"
    tools:context=".ResultListActivity">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            tools:layout_editor_absoluteX="8dp"
            tools:layout_editor_absoluteY="8dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <View
                    android:layout_width="match_parent"
                    android:layout_height="2dp"
                    android:layout_marginTop="150px"
                    android:background="@color/com_facebook_blue" />

                <TextView
                    android:id="@+id/textView20"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="결 과"
                    android:textColor="@color/accent_color"
                    android:textSize="30sp"
                    android:textStyle="bold" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="2dp"
                    android:layout_marginBottom="80px"
                    android:background="@color/com_facebook_blue" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="30dp"
                android:gravity="center"
                android:orientation="horizontal"
                android:paddingLeft="25dp"
                android:paddingRight="25dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_alignParentRight="true"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:paddingRight="15dp">

                    <ImageButton
                        android:id="@+id/button11"
                        android:layout_width="match_parent"
                        android:layout_height="135dp"
                        android:background="@color/blue"
                        android:onClick="btnClick"
                        android:scaleType="fitCenter"
                        android:text="Button"
                        app:srcCompat="@drawable/ic_call_white_48px" />

                    <TextView
                        android:id="@+id/textView21"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/blue"
                        android:gravity="center"
                        android:text="전화"
                        android:textColor="@color/cardview_light_background" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:paddingLeft="15dp">

                    <ImageButton
                        android:id="@+id/button12"
                        android:layout_width="match_parent"
                        android:layout_height="135dp"
                        android:background="@android:color/holo_orange_dark"
                        android:onClick="btnClick"
                        android:scaleType="fitCenter"
                        android:text="Button"
                        app:srcCompat="@drawable/ic_chat_white_48px" />

                    <TextView
                        android:id="@+id/textView22"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@android:color/holo_orange_dark"
                        android:gravity="center"
                        android:text="문자"
                        android:textColor="@color/cardview_light_background" />
                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="30dp"
                android:gravity="center"
                android:orientation="horizontal"
                android:paddingLeft="25dp"
                android:paddingRight="25dp">

                <LinearLayout
                    android:layout_width="400px"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="0dp"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:paddingRight="15dp">

                    <ImageButton
                        android:id="@+id/button13"
                        android:layout_width="match_parent"
                        android:layout_height="135dp"
                        android:layout_weight="1"
                        android:background="#ffeb02"
                        android:onClick="btnClick"
                        android:scaleType="fitCenter"
                        android:src="@drawable/kakaotalk_icon" />

                    <TextView
                        android:id="@+id/textView23"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="#fff22f"
                        android:gravity="center"
                        android:lines="1"
                        android:text="KakaoTalk"
                        android:textColor="@color/navigationBarColor" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="400px"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:paddingLeft="15dp">

                    <ImageButton
                        android:id="@+id/button14"
                        android:layout_width="match_parent"
                        android:layout_height="135dp"
                        android:background="@color/colorAccent"
                        android:onClick="btnClick"
                        android:paddingLeft="10dp"
                        android:scaleType="fitXY"
                        android:text="Button"
                        app:srcCompat="@drawable/ic_apps_white_48px" />

                    <TextView
                        android:id="@+id/textView24"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/colorAccent"
                        android:gravity="center"
                        android:text="어플리케이션"
                        android:textColor="@color/cardview_light_background" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="30dp"
                android:gravity="center"
                android:orientation="horizontal"
                android:paddingLeft="25dp"
                android:paddingRight="25dp">

                <LinearLayout
                    android:layout_width="400px"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="0dp"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:paddingRight="15dp">

                    <ImageButton
                        android:id="@+id/button15"
                        android:layout_width="match_parent"
                        android:layout_height="135dp"
                        android:background="@color/purple"
                        android:onClick="btnClick"
                        android:scaleType="fitCenter"
                        android:text="Button"
                        app:srcCompat="@drawable/ic_wb_sunny_white_48px" />

                    <TextView
                        android:id="@+id/textView25"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/purple"
                        android:gravity="center"
                        android:text="날씨"
                        android:textColor="@color/cardview_light_background" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="400px"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:paddingLeft="15dp">

                    <ImageButton
                        android:id="@+id/button16"
                        android:layout_width="match_parent"
                        android:layout_height="135dp"
                        android:background="@android:color/holo_green_light"
                        android:onClick="btnClick"
                        android:scaleType="fitCenter"
                        android:text="Button"
                        app:srcCompat="@drawable/ic_notifications_none_white_48px" />

                    <TextView
                        android:id="@+id/textView26"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@android:color/holo_green_light"
                        android:gravity="center"
                        android:text="알림"
                        android:textColor="@color/cardview_light_background" />
                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="30dp"
                android:gravity="center"
                android:orientation="horizontal"
                android:paddingLeft="25dp"
                android:paddingRight="25dp">

                <LinearLayout
                    android:layout_width="400px"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="0dp"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:paddingRight="15dp">

                    <ImageButton
                        android:id="@+id/button18"
                        android:layout_width="match_parent"
                        android:layout_height="135dp"
                        android:background="@android:color/holo_orange_light"
                        android:onClick="btnClick"
                        android:scaleType="centerCrop"
                        android:src="@drawable/naver"
                        android:text="Button" />

                    <TextView
                        android:id="@+id/textView28"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="#02c70a"
                        android:gravity="center"
                        android:text="NAVER"
                        android:textColor="@color/cardview_light_background" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="400px"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:paddingLeft="15dp">

                    <ImageButton
                        android:id="@+id/button19"
                        android:layout_width="match_parent"
                        android:layout_height="135dp"
                        android:background="@color/cyan"
                        android:onClick="btnClick"
                        android:scaleType="fitCenter"
                        android:text="Button"
                        app:srcCompat="@drawable/ic_settings_white_48px" />

                    <TextView
                        android:id="@+id/textView29"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/cyan"
                        android:gravity="center"
                        android:text="설정"
                        android:textColor="@color/cardview_light_background" />
                </LinearLayout>

            </LinearLayout>

        </LinearLayout>
    </ScrollView>

</android.support.constraint.ConstraintLayout>